  },
  "homepage": "https://github.com/HarnBark/FlowHunt#readme",
  "devDependencies": {
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "@babel/core": "^7.20.12",
